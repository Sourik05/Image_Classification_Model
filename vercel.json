{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {"runtime": "python3.10"}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10"
  },
  "scripts": {
    "build": "pip install -r requirements.txt"
  }
}
